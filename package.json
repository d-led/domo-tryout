{
  "name": "domo-tryout",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "postinstall": "node scripts/build-domo-actors.js",
    "build": "node build.js",
    "dev": "node build.js --watch",
    "serve": "npm run build && npx http-server dist -p 8000 -c-1",
    "server": "cd server && npm install && npm start",
    "test": "playwright test --reporter=list",
    "test:ui": "playwright test --ui",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "prettier": "prettier -w ."
  },
  "dependencies": {
    "@syncedstore/core": "^0.6.0",
    "domo-actors": "^1.0.2",
    "y-indexeddb": "^9.0.12",
    "y-webrtc": "^10.3.0",
    "y-websocket": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@vitest/ui": "^4.0.9",
    "esbuild": "^0.27.0",
    "http-server": "^14.1.1",
    "prettier": "^3.6.2",
    "vitest": "^4.0.9"
  }
}
